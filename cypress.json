{   
    "baseUrl": "https://inventa.shop",
    "viewportWidth": 1440,
    "viewportHeight": 900,
    "defaultCommandTimeout": 6000,
    "slowCommandTimeout": 6000,
    "chromeWebSecurity": false,
    "updateState": 6000,
    "updateTimeout": 6000,
    "waitForPageToLoadSafeTimeout": 6000,
    "scrollBehavior":"center",
